<script>
  
  function saveNotes(e){
    if (
      $("#note-trigger").is(":checked") &&
      checkAddGiftForm($("#note-wrap")) == false && checkAddGiftForm($("#note-wrap-pdp")) == false
    ) {
      return;
    } else if (
