<script>
// Display Toast Notification after 15secs
// Interval of 45secs between displays
$(function(){    
    setTimeout(function(){
        getBoughtNotification();
        if(2 > 1){
            runNotifications();
        } 
    }, 15000);              

    function runNotifications() {
        let count = 0;
        const intervalId = setInterval(() => {
            getBoughtNotification();
            if (++count === 2) clearInterval(intervalId);
        }, 45000);
    }
        
    function getBoughtNotification(){
        fetch('https://shopper.sugoimart.com/api/toast/notification?store_id=2')
            .then(response => response.json())
            .then(response => {              
                const Toast = Swal.mixin({
                toast: true,
                position: 'bottom-start',
                showConfirmButton: false,
                width: 400,
                showCloseButton: true,
                html: `
                    <div class="custom-flex" id="toast-notification" data-id="${response.data.id}">
                        <div class="custom-width">
                            <a href="${response.data.link}">
                                <img src="${response.data.image}" class="custom-img-toast">
                            </a>
                        </div>
                        <div class="custom-flex-col">
                            <a href="${response.data.link}">
                                <p class="custom-text-xl text-black">${response.data.title}</p>
                                <p class="custom-text-xl custom-font-bold text-body">${response.data.subtitle}</p>
                                ${response.data.show_time ? `<p class="custom-text-xl text-black">${response.data.created_at}</p>` : ''}
                            </a>
                        </div>
                    </div>
                    `,
                timer: 4000,
                timerProgressBar: true,
                didOpen: (toast) => {
                    toast.addEventListener('mouseenter', Swal.stopTimer)
                    toast.addEventListener('mouseleave', Swal.resumeTimer)
                }
            });
            Toast.fire()
        });
    }
});
</script>

<script>
// Track click event
document.addEventListener('DOMContentLoaded', function () {
    document.body.addEventListener('click', function (event) {
        var element = event.target.closest('#toast-notification');
        if (element) {
            var dataId = element.getAttribute('data-id');
            fetch('https://shopper.sugoimart.com/api/toast/notification', {
                method: 'POST',
                headers: {'Content-Type': 'application/json;charset=UTF-8'},
                body: JSON.stringify({
                    id: dataId,
                    store_id: 2
                })
            });
        }
    });
});
