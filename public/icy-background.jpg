<script>
  function wishlistModal() {
    return {
      isLoading: false,
      isOpen: false,
      products: [],
      quantity: 0,      
      currentPage: 1,
      last_page: 1,
      from: 0,
      to: 0,
      store: 2,
      emailConsent: false,
      customer_id: '',
      email: '',
      addingToCart: {},
      fetchWishlistProducts() {
        this.isLoading = true;
        if(this.customer_id){
          fetch('https://shopper.sugoimart.com/api/customer/wishlist?customer_id=&store_id='+this.store+'&email='+this.email+'&page='+this.currentPage)
          .then(response => response.json())
          .then(data => {
              this.products = data.data;              
              this.last_page = data.last_page;
              this.quantity = data.total;
              this.sku = data.sku;
              this.from = data.from;
              this.to = data.to;
              this.emailConsent = data.email_consent == 'y' ? true : false;
              this.isLoading = false;                    
          });           
        }else{
          window.location.replace("/account/login");
        }                         
      },
      closeProductQuickView() {
        this.isOpen = false;
        this.products = [];
      },
      previousPage(){
        if(this.currentPage >= 2){
            this.currentPage--;
            this.fetchWishlistProducts();
        }
      },
      nextPage(){
          if(this.currentPage < this.last_page){
              this.currentPage++;
              this.fetchWishlistProducts();
          }
      },
      setPage(page){       
        this.currentPage = page;
        this.fetchWishlistProducts();        
      },
      moneyFormat(value) {
        const formatter = new Intl.NumberFormat('en-US', {
          style: 'currency',
          currency: 'USD'
        });
        return formatter.format(value);
      },
      add(variant_id) {
        // Set the status to "Adding..." when the request starts
        this.addingToCart[variant_id] = 'Adding...';

        fetch('/cart/add.js', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(
            {quantity: 1, id: variant_id}
          )
        }).then((response) => {
          this.isLoading = false;
          document.dispatchEvent(new CustomEvent("cart:build"));

          // Set the status to "Added" after the request completes
          this.addingToCart[variant_id] = 'Added';
          
          // Optionally, reset the status after a few seconds
          setTimeout(() => {
            this.addingToCart[variant_id] = null;
          }, 3000);
        }).catch((error) => {
          this.addingToCart[variant_id] = null;
        })
      },
      removeWishlistItem(sku) {
        this.isLoading = true;
        this.products = [];
        fetch('https://shopper.sugoimart.com/api/customer/wishlist/destroy', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(
            {sku: sku, customer_id: this.customer_id, store_id: this.store}
          )
        }).then((response) => {                   
          this.fetchWishlistProducts();    
        }).catch((error) => {}) 
      },
      addToWishlist(sku){    
        this.isLoading = true;                
        fetch('https://shopper.sugoimart.com/api/customer/wishlist', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(
            {sku: sku, customer_id: this.customer_id, store_id: this.store, email: this.email}
          )
        }).then((response) => {
          this.currentPage = 1;
          this.fetchWishlistProducts();
        }).catch((error) => {})        
      },
      confirmClearWishlist() {
        if (confirm('Are you sure you want to clear your wishlist?')) {
          this.clearWishlistItems();
        }
      },
      clearWishlistItems() {
        this.isLoading = true;
        fetch('https://shopper.sugoimart.com/api/customer/wishlist/destroy_all', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({customer_id: this.customer_id, store_id: this.store})
        }).then((response) => {
          this.currentPage = 1;
          this.isLoading = false;
        }).catch((error) => {}) 
          this.quantity = 0;
          this.last_page = 0;
          this.currentPage = 1;
          this.products = [];
          this.isLoading = false;
      },
      changeEmailConsent() {
        this.isLoading = true;
        fetch('https://shopper.sugoimart.com/api/email/wishlist/consent', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({email: this.email, store_id: this.store})
        }).then((response) => {
          this.isLoading = false;
        }).catch((error) => {}) 
          this.isLoading = false;
      }    
    };
  }
</script>

    

