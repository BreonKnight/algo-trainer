  <script>
    var storeUrl=Shopify.shop;
    var REBUY_API_KEY;

    if(storeUrl==='japancratestore.myshopify.com')
    {
      REBUY_API_KEY='4318b26cda0fd959661ab467254d6519576c326b';
    }
    else if(storeUrl==='bokksugrocery.myshopify.com')
    {
      REBUY_API_KEY='8000c29e79ae4b4a117228c4bcaf89e80ce175af';
    }
    else if(storeUrl==='bokksu.myshopify.com')
    {
      REBUY_API_KEY='544602285c99b524bbea03795786138a9f7a2097';
    }
    else if(storeUrl==='b-grocery-staging.myshopify.com')
    {
      REBUY_API_KEY='edb7783ba066f34661d0b9674e8187e9518c0c61';
    }else if (storeUrl === "japancrate-store.myshopify.com") 
    {
      REBUY_API_KEY = "f2e527f738ff84494a7acb56d63fbc5ee5fc4816";
    }

    requestParameters = new URLSearchParams(
      {
        format: "pretty",
        limit: 10
      }
    );

    fetch(`https://rebuyengine.com/api/v1/products/recommended?key=${REBUY_API_KEY}&${requestParameters}`).then(response => response.json()).then(
      (res) => {
        let output = ''
        const filteredProducts = res.data.filter(
          product => product.title.length <= 35 && product.variants[
            0].inventory_quantity > 0 && !product.title.includes(
            'Discovery Pack')
        ).slice(0, 3);

        for(let i = 0; i < filteredProducts.length; i++)
        {
          let product = filteredProducts[i]
          // convert discounted and original prices  into superscripts
          let superScriptedDiscountedPrice = product.variants[0].price.toString().split('.').join('<sup>')
          +'</sup>';
          var superScriptedOriginalPrice = product.variants[0].compare_at_price ? '$'+product.variants[0].compare_at_price.toString().split('.').join(
            '<sup id="super-scrip">')
          +'</sup>': '';

          output += `<div class="img-text-container">
                <img class="empty-cart-img" src="${product.images[0].src}" alt="${product.images[0].alt}">
                <div class="img-title-price">
                    <h2 class="img-title">${product.title}</h2>
                    <div class="empty-cart-prices">
                        <span class="discounted-price">$${superScriptedDiscountedPrice}</span>
                        <span class="strike-through-price">${superScriptedOriginalPrice}</span>
                    </div>
                </div>
                <div class="empty-cart-plus-icon">

                      <button type="button" class="text-link quick-add-btn js-quick-add-btn" tabindex="-1" data-id=${product.id}  onclick="addToCartSection1(${
          product.id},${product.variants[0].id})" data-id="${product.variants[0].id}">
                        <span class="cirle-btn">
                          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                            <path d="M12 5V19" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M5 12H19" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                            </svg>
                          <span class="icon__fallback-text">Add to cart</span>
                        </span>
                    </button>
               </div>
             </div>`

        }

        $('#empty-cart-rebuy-products').html(output)
        $('#empty-cart-rebuy-products').css({"display": "flex", "flex-direction": "column", "gap": "24px"})
      }
    )
  </script></div>
