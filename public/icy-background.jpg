        // Get checkout items from the checkout object        
        function getCheckoutItems() {
            if(typeof Shopify !== 'undefined' && Shopify.Checkout){
                let checkoutItems = [
                    
                ];
                return checkoutItems;
            }
        }
        
        // Get only the fields we need from the cart items
        function extractSelectedFields(cart) {            
            return cart.map(item => {
                return {
                    variant_id: item.id,
                    product_id: item.product_id,
                    sku: item.sku,
                    title: item.title,
                    quantity: item.quantity,
                    price: checkAndConvert(item.price),
                    vendor: item.vendor,
                    category: item.product_type
                };
            });
        }

        // Function to generate unique event_id
        function generateEventId() {
            const timestamp = Date.now(); // Current timestamp in milliseconds
            const randomNum = Math.floor(Math.random() * 1000000); // Random number between 0 and 999999
            return `${timestamp}_${randomNum}`;
        }

        /* Make item object */
        const itemObject = async (itemData, quantity) => {
            return {
                product_id: itemData.product.id,
                id: itemData.variant.id,
                handle: itemData.product.handle,
                quantity: quantity,
                price: itemData.variant.price,
                compare_price: itemData.variant.compare_at_price,
                sku: itemData.variant.sku,                
                image: itemData.product.images[0]?.src || '',
                url: `/products/${itemData.product.handle}`,                
                title: itemData.product.title
            };
        };

        const sumQuantities = (items) => {
            return items.reduce((total, item) => {
                return total + (item.quantity || 0);
            }, 0);
        };
        
        async function fetchProductWithVariant(handle, variantId) {
            try {
                const response = await fetch(`/products/${handle}.json`);
                const data = await response.json();
                const product = data.product;
                let variant = product.variants.find(variant => variant.id == variantId);
                return { product, variant };
            } catch (error) {
                console.error('Error:', error);
            }
        }

        async function fetchCartItems() {
            // Create a delay function to wait for 2 seconds
            function delay(ms) {
                return new Promise(resolve => setTimeout(resolve, ms));
            }

            try {
                // Wait for 2 seconds before fetching the cart data
                await delay(2000);

                const response = await fetch(`/cart.json`);
                const data = await response.json();

                // Filter out the item with sku 'ONWARDINS01'
                const filteredItems = data.items.filter(item => item.sku !== 'ONWARDINS01');
                
                return filteredItems;
            } catch (error) {
                console.error('Error:', error);
            }
        }

        function createEcommerceEntry(item, cartItems, quantityAdjustment = 0) {
            return {
                'items': [{
                    'item_id'         : item.product_id,
                    'item_variant'    : item.id,
                    'item_name'       : item.title,                    
                    'price'           : checkAndConvert(item.price),
                    'compare_price'   : checkAndConvert(item.compare_at_price),
                    'item_brand'      : storeAbbr,
                    'item_category'   : item.type,
                    'vendor'          : item.vendor,
                    'quantity'        : Number(item.quantity) + Number(quantityAdjustment),                    
                    'image_url'       : item.image,
                    'product_url'     : item.url,
                    'sku'             : item.sku,
                    'customer_id'     : null,
                    'customer_email'  : null,                                                
                }],
                'cart': {
                    'cart_items'      : extractSelectedFields(cartItems),
                    'cart_quantity'   : sumQuantities(cartItems),
                    'cart_subtotal'   : checkAndConvert(cartSubtotal(cartItems))      
                }
            };
        }                         
        return {
            userData: function () {
                // Customer data object
                let customerData = {
                    is_logged_in: false,
                    type: 'guest',
                    id: null,
                    email: null,
                    first_name: null,
                    last_name: null,
                    phone_number: null,
                    accepts_marketing: null,
                    total_spent: null,
                    orders_count: null,
                    default_address: null
                };

                // Liquid code to get customer data if logged in
                

                // Push the customer data to the dataLayer
                window.dataLayer.push({
                    'event': 'dl_user_data',
                    'user_data': customerData,
                    'marketing': getUTMParameters()
                });
            },                                    
            viewItem: function() {               
                // SKU ONWARDINS01 must be ignored         
                let ecommerce = {
                    'items': [{
                        'item_id'         : null,
                        'item_variant'    : null,
                        'item_name'       : null,                        
                        'price'           : "",
                        'item_brand'      : storeAbbr,
                        'item_category'   : null,                                             
                        'vendor'          : null,         
                        'image_url'       : 'https://sugoimart.com/cdn/shopifycloud/shopify/assets/no-image-2048-5e88c1b20e087fb7bbe9a3771824e743c244f437e4f8ba93bbf7b11b53f7824c_grande.gif',
                        'product_url'     : '',
                        'sku'             : null                                                        
                    }],
                    'cart': { 
                        'cart_items'      : getCartItems(),
                        'cart_quantity'   : sumQuantities(getCartItems()),
                        'cart_subtotal'   : cartSubtotal(getCartItems()),
                    }
                };
                            
                dataLayer.push({
                    'pageType' : 'Product',
                    'event'    : 'dl_view_item',
                    'event_id': generateEventId(),
                     ecommerce
                });        
                    
            },
            viewCollection: function() {      
                                   
                let ecommerce = {
                    'collection': {
                        'id'              : null,
                        'handle'          : null,
                        'description'     : null,
                        'products_count'  : null,                                                                            
                        'products'  : [
                            
                        ],                                                    
                    },
                    'cart': { 
                        'cart_items'      : getCartItems(),
                        'cart_quantity'   : sumQuantities(getCartItems()),
                        'cart_subtotal'   : cartSubtotal(getCartItems()),
                    }
                };               
                dataLayer.push({
                    'pageType' : 'Collection',
                    'event'    : 'dl_view_item_list',
                    'event_id': generateEventId(),
                     ecommerce
                });            
            },
            addToCart: async function(handle, variantId, quantity, quantityAdjustment = 1) {
                this.userData();
                const itemData = await fetchProductWithVariant(handle, variantId);
                const currentCartItems = await fetchCartItems();

                const item = await itemObject(itemData, quantity);
                
                // SKU ONWARDINS01 must be ignored
                if (item.sku == 'ONWARDINS01') {
                    return;
                }

                const ecommerce = createEcommerceEntry(item, currentCartItems, quantityAdjustment);
                dataLayer.push({
                    'event': 'dl_add_to_cart',
                    'event_id': generateEventId(),
                    ecommerce
                });
            },
            removeFromCart: async function(handle, variantId, quantity, quantityAdjustment = -1) {
                this.userData();
                const itemData = await fetchProductWithVariant(handle, variantId);
                const currentCartItems = await fetchCartItems();
                const item = await itemObject(itemData, quantity);

                // SKU ONWARDINS01 must be ignored
                if (item.sku == 'ONWARDINS01') {
                    return;
                }

                const ecommerce = createEcommerceEntry(item, currentCartItems, quantityAdjustment);
                dataLayer.push({
                    'event': 'dl_remove_from_cart',
                    'event_id': generateEventId(),
                    ecommerce
                });
            },            
            checkout: function() {
                this.userData();
                if(typeof Shopify !== 'undefined' && Shopify.Checkout){
                    let ecommerce = {
                        'items': [{
                            'transaction_id': null,
                            'order_id': null,
                            'customer_id': null,
                            'affiliation': "Sugoi Mart by Japan Crate ",
                            'value': "",
                            'tax': "",
                            'shipping': "",
                            'subtotal': "",
                            'currency': null,
                            
                            'email': null,                                                      
                        }],
                        'cart': {
                            'cart_items': getCheckoutItems(),
                            'cart_quantity'   : sumQuantities(getCheckoutItems()),
                        }
                    };
            
                    if(Shopify.Checkout.step){ 
                        if(Shopify.Checkout.step.length > 0){
                            if (Shopify.Checkout.step === 'contact_information'){
                                dataLayer.push({
                                    'event'    :'dl_begin_checkout',
                                    'pageType' :'Customer Information',
                                    'step': 1,
                                    'event_id': generateEventId(),
                                    ecommerce
                                });
                            }else if (Shopify.Checkout.step === 'shipping_method'){
                                dataLayer.push({
                                    'event'    :'add_shipping_info',
                                    'pageType' :'Shipping Information',
                                    'event_id': generateEventId(),
                                    ecommerce
                                });
                            }else if( Shopify.Checkout.step === "payment_method" ){
                                dataLayer.push({
                                    'event'    :'add_payment_info',
                                    'event_id': generateEventId(),
                                    'pageType' :'Add Payment Info',
                                    ecommerce
                                });
                            }
                        }
                                    
                        if(Shopify.Checkout.page == "thank_you"){
                            dataLayer.push({
                            'pageType' :'Transaction',
                            'event_id': generateEventId(),
                            'event'    :'dl_purchase',
                            ecommerce
                            });
                        }               
                    }
                }
            },            
        };
    })();

    /*
        EVENTS ADD TO CART, REMOVE FROM CART, PURCHASE, PDP PAGE VIEW, CHECKOUT
    */

    /* User Data */
    dataLayerHelper.userData();
      
