    /* Add and Remove cart items (CART and PDP minus and plus)*/
    document.addEventListener('click', async function(event) {
        const button = event.target.closest('.js-qty__wrapper button');
        if (button) {
            const input = button.closest('.js-qty__wrapper').querySelector('input');
            if (input) {
                const handle = input.getAttribute('data-product-handle');
                const variantId = input.getAttribute('data-variant-id');
                
                if (button.classList.contains('js-qty__adjust--minus')) {
                    await dataLayerHelper.removeFromCart(handle, variantId, input.value, 0);
                } else if (button.classList.contains('js-qty__adjust--plus')) {            
                    await dataLayerHelper.addToCart(handle, variantId, input.value, 0);
                }
            }
        }
    });
    
    /* Add and Remove cart items (HOMEPAGE) */
    document.addEventListener('click', async function(event) {
        const target = event.target.closest('.qty-custom-plus, .qty-custom-minus, .quick_add_to_cart');
        if (!target) return;

        const gridItem = target.closest('.grid-item');
        if (!gridItem) return;

        const handle = gridItem.getAttribute('data-product-handle');
        const variantId = gridItem.getAttribute('data-variant-id'); 

        const quantitySpan = gridItem.querySelector('.custom-qty');
        let quantity = 0;
        if (quantitySpan){
            quantity = parseInt(quantitySpan.textContent);       
        };

        if (target.classList.contains('qty-custom-minus')) {
            await dataLayerHelper.removeFromCart(handle, variantId, quantity, -1);
        } else if (target.classList.contains('qty-custom-plus')) {
            await dataLayerHelper.addToCart(handle, variantId, quantity, 1);        
        } else if (target.classList.contains('quick_add_to_cart')) {
            await dataLayerHelper.addToCart(handle, variantId, 1, 0);
        }
    });

    /* Add button cart (Customers Also Loved) & add button cart normal state (PDP)*/
    document.addEventListener('click', async function(event) {
        const button = event.target.closest('#rebuy-product button, .pdp-atc');
        if (button) {                                   
            const handle = button.getAttribute('data-product-handle');
            const variantId = button.getAttribute('data-variant-id');
            await dataLayerHelper.addToCart(handle, variantId, 1, 0);
        }
    });   

    /* Collection Single button and plus and minus using FS*/
    window.addEventListener('fs-custom-events-product-clicked', (e) => {
        if(e && e.detail && e.detail.id){
            const productId = e.detail.id                    
            const handle = e.detail.productURL.split('/').pop();
            const variantId = e.detail.s.split('::').pop();         

            const shadow = document.querySelector('#fast-simon-serp-app') && document.querySelector('#fast-simon-serp-app').shadowRoot ? document.querySelector('#fast-simon-serp-app').shadowRoot:null;
            if(shadow){
                if(shadow.querySelector(`.fs-results-product-card[data-product-id="`+productId+`"]`)){
                const element = shadow.querySelector(`.fs-results-product-card[data-product-id="`+productId+`"]`);
                if(element.querySelector(".add-item-to-cart .qty-custom-minus")){                    
                    element.querySelector(".add-item-to-cart .qty-custom-minus").addEventListener("click", function () {
                        const quantitySpan = element.querySelector('.custom-qty');
                        let quantity = 0;
                        if (quantitySpan){
                            quantity = parseInt(quantitySpan.textContent);       
                        };
                        dataLayerHelper.addToCart(handle, variantId, quantity, -1);                     
                    });
                } 

                if(element.querySelector(".add-item-to-cart .qty-custom-plus")){
                    element.querySelector(".add-item-to-cart .qty-custom-plus").addEventListener("click", function () {
                        const quantitySpan = element.querySelector('.custom-qty');
                        let quantity = 0;
                        if (quantitySpan){
                            quantity = parseInt(quantitySpan.textContent);       
                        };
                        dataLayerHelper.addToCart(handle, variantId, quantity, 1);                   
                    });
                }
                
                if(element.querySelector(".qty-custom-add")){
                    dataLayerHelper.addToCart(handle, variantId, 1, 0);
                }
                }
            }          
       }        
    });
