          const TAG_FACET_NAME = "Tag";
          const NEW_TAG_NAME = ["New Arrival","NEW ARRIVAL","new arrival"];
          const SALE_TAG_NAME = ["Sale", "SALE","sale"];
          const STAFF_TAG_NAME = ["Staff Pick","STAFF PICK","staff pick"];
          const BESTSELLER_TAG_NAME = ["Best Seller","BEST SELLER", "best seller"];
          const MKT_FROZEN_TAG_NAME = ["MKT_frozen","MKT_FROZEN"];
          const LIMITED_EDITION_TAG_NAME = ["Limited Edition","LIMITED EDITION","limited edition"];
          const PREORDER_TAG_NAME = ["preorder","PREORDER"];
          const OCCASIONAL_BADGE = ["PRIORITY_SHIP_OK"];
    
      
        
          function getProductElements(element) {
             return [...element.querySelectorAll(`.fs-results-product-card`)];
          }
          const NewBadges = {
      
              set: (product, productElement) => {
                  
                  var $badgewapper = `<div class="fs-badges-wrapper"></div>`;
      
                  if (productElement.querySelectorAll(`.fs-badges-wrapper`).length === 0 && productElement.querySelector(".image-wrapper")) {
                      productElement.querySelector(".image-wrapper").insertAdjacentHTML('afterBegin', $badgewapper);
                  }
                  const tags = product.tags;
      
                  /* Sale */
                  if(NewBadges.isOnSale(tags) && !NewBadges.isMKT_FROZEN(tags)) {
                      if(product.price && product.comparePrice){
                        const originalDiscount = (product.comparePrice - product.price) * 100 / product.comparePrice;
                        const roundedDiscount = NewBadges.customRound(originalDiscount);  
                        NewBadges.addLabel(productElement,`<div class="fs_product_text_badge_flag fs_sale-sticker cro-p-cards"> <span class="fs_sticker-text">${roundedDiscount}% OFF</span></div>`);
                      }
                  }
                  else if(NewBadges.isNewEdition(tags) && !NewBadges.isOccasionalBadge(tags)){
                      NewBadges.addLabel(productElement,'<div class="fs_product_text_badge_flag fs_new-sticker"> <span class="fs_sticker-text">NEW</span></div>');
                  } 
                  else if(NewBadges.isBestSeller(tags) && !NewBadges.isOccasionalBadge(tags)) {
                      NewBadges.addLabel(productElement,'<div class="fs_product_text_badge_flag fs_best-seller-sticker"> <span class="fs_sticker-text">BEST SELLER</span></div>');
                  } 
             
              
                  else if(NewBadges.isStaffPick(tags) && !NewBadges.isOccasionalBadge(tags)) {
                      NewBadges.addLabel(productElement,'<div class="fs_product_text_badge_flag fs_staff-pick-sticker"> <span class="fs_sticker-text">STAFF PICK</span></div>');
                  } 
                  else if(NewBadges.isMKT_FROZEN(tags) && !NewBadges.isOccasionalBadge(tags)) {
                    NewBadges.addLabel(productElement,'<div class="fs_product_text_badge_flag fs_frozen-sticker"> <span class="fs_sticker-text">FROZEN</span></div>');
                  }
                   if(NewBadges.isLimitedEdition(tags) && !NewBadges.isOccasionalBadge(tags) ) {
                    NewBadges.addLabel(productElement,'<div class="fs_product_text_badge_flag fs_limited-edition-sticker"> <span class="fs_sticker-text">LIMITED EDITION</span></div>');
                  }
                    if(NewBadges.isPreorder(tags)) {
                    NewBadges.addLabel(productElement,'<div class="fs_product_text_badge_flag fs_preorder-sticker"> <span class="fs_sticker-text">PREORDER</span></div>');
                  }
                  
              },
      
              customRound:(discount)=>{
                  return Math.round(discount)
              },
            
              addLabel: (productElement, promolabel ) => {
                  const label = productElement.querySelector(`.fs_product_text_badge_flag`);
                  if (label) {
                      label.remove();
                  }
                  productElement.querySelector(".fs-badges-wrapper").insertAdjacentHTML('afterBegin', promolabel);
              },
      
      
      
              isNewEdition: (tags) => {
                  return  NEW_TAG_NAME.some(tagName => tags.includes(tagName));
              },
              
              isOnSale: (tags) => {
              return SALE_TAG_NAME.some(tagName => tags.includes(tagName));
              },
              
              isStaffPick: (tags) => {
                  return STAFF_TAG_NAME.some(tagName => tags.includes(tagName));
      
              },
      
              isBestSeller: (tags) => {
          return BESTSELLER_TAG_NAME.some(tagName => tags.includes(tagName));
      },
              
      
        isMKT_FROZEN : (tags) => {
          return MKT_FROZEN_TAG_NAME.some(tagName => tags.includes(tagName));
      },
        isLimitedEdition : (tags) => {
          return LIMITED_EDITION_TAG_NAME.some(tagName => tags.includes(tagName));
      },
        isPreorder : (tags) => {
          return PREORDER_TAG_NAME.some(tagName => tags.includes(tagName));
      },
      isOccasionalBadge: (tags) => {
        return OCCASIONAL_BADGE.some(tagName => tags.includes(tagName));
      }
    }
    
