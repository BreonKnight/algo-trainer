{
  "name": "algo-trainer",
  "private": true,
  "version": "1.0.0",
  "description": "Algorithm Trainer",
  "author": "Breon",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/copy-dependencies.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "electron:dev": "tsc -p electron/tsconfig.json && NODE_ENV=development electron .",
    "electron:build": "npm run build && tsc -p electron/tsconfig.json && electron-builder",
    "electron:preview": "npm run build && tsc -p electron/tsconfig.json && NODE_ENV=production electron .",
    "extract-patterns": "ts-node scripts/extract-patterns.ts",
    "create-implementations": "node scripts/create-implementations.js",
    "split-pseudocode": "tsx scripts/split-pseudocode.ts",
    "audit-patterns": "tsx src/lib/pseudocode/scripts/audit-patterns.ts",
    "validate-naming": "tsx src/components/algorithm-trainer/scripts/validate-naming.ts",
    "fix-naming": "tsx src/components/algorithm-trainer/scripts/fix-naming.ts",
    "check-patterns": "ts-node scripts/check-patterns.ts",
    "update-prism": "tsx scripts/update-prism.ts",
    "preinstall": "npx npm-force-resolutions",
    "validate": "ts-node src/components/algorithm-trainer/validatePatterns.ts",
    "generate-icons": "node scripts/generate-icons.js",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "postbuild": "node scripts/copy-pyodide.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@floating-ui/react-dom-interactions": "^0.13.3",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@tanstack/react-query": "^5.76.2",
    "@tanstack/react-query-devtools": "^5.76.2",
    "@tanstack/react-virtual": "^3.13.8",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/three": "^0.176.0",
    "axios": "^1.9.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "highlight.js": "^10.7.3",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.294.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "1.30.0",
    "pyodide": "^0.27.6",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^7.5.3",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.176.0",
    "typescript": "^5.0.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.2",
    "@babel/traverse": "^7.27.1",
    "@babel/types": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.44",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.2",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "vite": "^6.3.2",
    "vite-plugin-static-copy": "^2.3.1"
  },
  "build": {
    "appId": "com.breon.algotrainer",
    "productName": "Algorithm Trainer",
    "mac": {
      "category": "public.app-category.education",
      "icon": "build/icons/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons/icon.png"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "extraResources": [
      {
        "from": "dist/monaco-editor",
        "to": "monaco-editor",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/pyodide",
        "to": "pyodide",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "asar": true,
    "asarUnpack": [
      "dist/pyodide/**/*",
      "dist/monaco-editor/**/*",
      "dist/assets/**/*",
      "dist/practice/**/*",
      "dist/tutorials/**/*",
      "dist/cs-math/**/*"
    ]
  },
  "config": {
    "sharp": {
      "platform": "darwin",
      "arch": "arm64"
    }
  },
  "overrides": {
    "refractor": {
      "prismjs": "1.30.0"
    },
    "prism-react-renderer": {
      "prismjs": "1.30.0"
    }
  }
}
